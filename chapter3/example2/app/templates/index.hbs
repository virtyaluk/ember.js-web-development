<table id="to1">
	<tr>
		<th>Company name</th>
		<th>Headquarters</th>
		<th>Revenue</th>
	</tr>
	{{#each model as |item|}}
		<tr>
			<td {{action "alert" "alert from company name"}}>{{item.name}}</td>
			<td {{action "alert" "alert from company headquarters"}} class={{className}}>{{item.headquarters}}</td>
			<td {{action "alert" "alert from company revenue"}}>{{item.revenue}}</td>
		</tr>
	{{/each}}
</table>
<button {{action "toggleColor"}}>Change color</button>